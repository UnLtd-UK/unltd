{
  "name": "unltd",
  "compatibility_date": "2025-11-17",
  "main": "./dist/worker/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS",
    "not_found_handling": "404-page"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "workers_dev": true,
  "vars": {
    "DEV": "true",
    "SHOW_DRAFTS": "true"
  },
  "env": {
    "production": {
      "vars": {
        "DEV": "false",
        "SHOW_DRAFTS": "false"
      },
      "routes": [
        {
          "pattern": "unltd.org.uk",
          "custom_domain": true
        }
      ]
    },
    "draft": {
      "vars": {
        "DEV": "false",
        "SHOW_DRAFTS": "true"
      },
      "routes": [
        {
          "pattern": "draft.unltd.org.uk",
          "custom_domain": true
        }
      ]
    },
    "preview": {
      "vars": {
        "DEV": "true",
        "SHOW_DRAFTS": "true"
      },
      "routes": [
        {
          "pattern": "preview.unltd.org.uk",
          "custom_domain": true
        }
      ]
    }
  }
}
